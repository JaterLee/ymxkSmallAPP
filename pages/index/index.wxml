<view class="container">
  <view class="banner">
    <swiper indicator-dots="true" autoplay="true">
      <block wx:for="{{list}}">
        <swiper-item>
          <image src="{{item.thumbnailURLs[0]}}" class="bannerImage" wx:if="{{item.description.length>0}}"/>
        </swiper-item>
      </block>
    </swiper>
  </view>
</view>